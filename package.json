  "main": "index.js",
  "scripts": {
    "analyze": "cross-env NODE_ENV=production webpack --analyze -c webpack/analyze.js",
    "build:development": "cross-env NODE_ENV=development webpack -c webpack/dev.js",
    "build:production": "cross-env NODE_ENV=production webpack -c webpack/prod.js",
    "start": "webpack serve --hot -c webpack/dev.js",
    "build": "npm run build:production",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier": "prettier --write src/**/*",
    "lint:style": "stylelint \"src/**/*.scss\" --syntax scss",
    "lint:js": "eslint \"src/**/*.js\"",
    "lint-fix": "npm run lint:js --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{scss,sass}": [
      "npm run lint:style"
    ],
    "src/**/*.{js,jsx}": [
      "npm run lint:js"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 3 versions",
    "not ie <= 10"
